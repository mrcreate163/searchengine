# Database Configuration
MYSQL_ROOT_PASSWORD=your_strong_root_password_here
MYSQL_DATABASE=searchengine
MYSQL_USER=searchengine_user
MYSQL_PASSWORD=your_strong_password_here

# Application Configuration
SPRING_PROFILES_ACTIVE=prod

# Indexing Sites Configuration
# Format: JSON array of objects with 'url' and 'name' fields
# Example: [{"url":"https://example.com",
"name":"Example Site"},{"url":"https://another.com","name":"Another Site"}]
INDEXING_SITES=[{"url":"https://www.skillbox.ru","name":"Skillbox"},{"url":"https://www.playback.ru","name":"PlayBack.Ru"}]

# Domain Configuration (used in nginx config)
DOMAIN_NAME=yourdomain.com
SSL_EMAIL=your-email@example.com
